{
   "action": {
      "default_icon": {
         "128": "icons/128.png",
         "16": "icons/16.png",
         "32": "icons/32.png",
         "48": "icons/48.png",
         "92": "icons/92.png"
      }
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "description": "I cast dark mode",
   "host_permissions": [ "file:///*.pdf" ],
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "92": "icons/92.png"
   },
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "PDF Dark Mode",
   "permissions": [ "scripting", "declarativeContent" ],
   "content_scripts":[{
      "matches":["<all_urls>"],
      "js":["background.js"]
      
   }],
   "version": "3.0"
}
